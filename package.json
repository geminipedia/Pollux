    "dotenv-cli": "^2.0.0",
    "graphql-yoga": "^1.16.9",
    "heml": "^1.1.3",
    "html2plaintext": "^2.1.2",
    "jsonwebtoken": "^8.4.0",
    "nodemailer": "^6.2.1",
    "prisma": "^1.34.1",
    "prisma-client-lib": "^1.34.1",
